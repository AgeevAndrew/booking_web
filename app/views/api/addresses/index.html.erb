<p id="notice"><%= notice %></p>

<h1>Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>City</th>
      <th>Street</th>
      <th>House</th>
      <th>Office</th>
      <th>Entrance</th>
      <th>Floor</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.title %></td>
        <td><%= address.city %></td>
        <td><%= address.street %></td>
        <td><%= address.house %></td>
        <td><%= address.office %></td>
        <td><%= address.entrance %></td>
        <td><%= address.floor %></td>
        <td><%= address.code %></td>
        <td><%= link_to 'Show', address %></td>
        <td><%= link_to 'Edit', edit_address_path(address) %></td>
        <td><%= link_to 'Destroy', address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address', new_address_path %>
